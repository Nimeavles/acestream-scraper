    "info": {
        "title": "Acestream Scraper API",
        "version": "1.0",
        "description": "REST API for Acestream channels and configurations"
    },
    "tags": [
        {
            "name": "stats",
            "description": "Application statistics"
        },
        {
            "name": "config",
            "description": "Configuration management"
        },
        {
            "name": "channels",
            "description": "Channel management"
        },
        {
            "name": "urls",
            "description": "URL management"
        },
        {
            "name": "health",
            "description": "Health check endpoints"
        },
        {
            "name": "playlists",
            "description": "Playlist management operations"
        },
        {
            "name": "warp",
            "description": "Cloudflare WARP management"
        }
    ],
    "servers": [
        {
            "url": "/api"
        }
    ],
    "components": {
        "responses": {
            "ParseError": {
                "description": "When a mask can't be parsed"
            },
            "MaskError": {
                "description": "When any error occurs on mask"
            }
        },
        "schemas": {
            "Stats": {
                "properties": {
                    "urls": {
                        "type": "array",
                        "description": "List of all tracked URLs",
                        "items": {
                            "$ref": "#/components/schemas/URLStats"
                        }
                    },
                    "total_channels": {
                        "type": "integer",
                        "description": "Total number of channels"
                    },
                    "channels_checked": {
                        "type": "integer",
                        "description": "Number of channels with status checked"
                    },
                    "channels_online": {
                        "type": "integer",
                        "description": "Number of online channels"
                    },
                    "channels_offline": {
                        "type": "integer",
                        "description": "Number of offline channels"
                    },
                    "base_url": {
                        "type": "string",
                        "description": "Base URL for playlist generation"
                    },
                    "ace_engine_url": {
                        "type": "string",
                        "description": "URL of the Acestream Engine"
                    },
                    "rescrape_interval": {
                        "type": "integer",
                        "description": "Hours between automatic rescans"
                    },
                    "addpid": {
                        "type": "boolean",
                        "description": "Whether to add PID parameter to URLs"
                    },
                    "task_manager_status": {
                        "type": "string",
                        "description": "Status of the background task manager"
                    }
                },
                "type": "object"
            },
            "URLStats": {
                "properties": {
                    "url": {
                        "type": "string",
                        "description": "URL being scraped"
                    },
                    "status": {
                        "type": "string",
                        "description": "Current status of the URL"
                    },
                    "last_processed": {
                        "type": "string",
                        "format": "date-time",
                        "description": "When the URL was last processed"
                    },
                    "channel_count": {
                        "type": "integer",
                        "description": "Number of channels from this URL"
                    },
                    "enabled": {
                        "type": "boolean",
                        "description": "Whether this URL is enabled"
                    },
                    "error_count": {
                        "type": "integer",
                        "description": "Number of consecutive errors"
                    },
                    "last_error": {
                        "type": "string",
                        "description": "Last error message, if any"
                    }
                },
                "type": "object"
            },
            "BaseURL": {
                "required": [
                    "base_url"
                ],
                "properties": {
                    "base_url": {
                        "type": "string",
                        "description": "Base URL for acestream links"
                    }
                },
                "type": "object"
            },
