    "paths": {
        "/channels/": {
            "get": {
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Channel"
                                    }
                                }
                            }
                        }
                    }
                },
                "summary": "Get list of channels, optionally filtered by search term",
                "operationId": "list_channels",
                "parameters": [
                    {
                        "name": "search",
                        "in": "query",
                        "description": "Filter channels by name",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Page number",
                        "schema": {
                            "type": "integer",
                            "default": 1
                        }
                    },
                    {
                        "name": "per_page",
                        "in": "query",
                        "description": "Items per page",
                        "schema": {
                            "type": "integer",
                            "default": 25
                        }
                    },
                    {
                        "name": "status",
                        "in": "query",
                        "description": "Filter by status",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "X-Fields",
                        "in": "header",
                        "description": "An optional fields mask",
                        "schema": {
                            "type": "string",
                            "format": "mask"
                        }
                    }
                ],
                "tags": [
                    "channels"
                ]
            },
            "post": {
                "responses": {
                    "201": {
